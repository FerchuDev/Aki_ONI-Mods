msgid "A fierce beast of legends."
msgstr "Una bestia feroz de leyenda."

#
msgctxt "MoreMarbleSculptures.STRINGS.BUILDINGS.PREFABS.MARBLESCULPTURE.DRAGON.NAME"
msgid "<link=\"MARBLESCULPTURE\">Dragon</link>"
